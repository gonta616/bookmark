{% extends '@App/base.html.twig' %}

{% block content %}
<div class="max-width">
    <div class="row">
        <div class="col-md-8 col-sm-12">

        </div>
        <div class="col-md-4">
            
        </div>
    </div>
</div>
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
        '@AppBundle/Resources/public/app/css/draft/index.css'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block javascript %}
    {{ parent() }}
    {% javascripts
        '@AppBundle/Resources/public/app/js/draft/index.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
